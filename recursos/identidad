    <script>
        const themes = {
            nature: {
                tagline: "Tu Copiloto digital con IA",
                description: "Soy la perfecta fusión entre tecnología Apple y equilibrio natural. Con mis ojos verde montaña y mi filosofía 'Tech meets Nature', transformo la comunicación empresarial sin perder la esencia humana.",
                cta: "🌿 Conoce a tu copiloto Natural"
            },
            ocean: {
                tagline: "Serenidad y confianza digital",
                description: "Transmito la profundidad y confianza del océano en cada interacción empresarial. Con mis ojos azul mar, aporto serenidad profesional y decisiones sólidas como las corrientes profundas.",
                cta: "🌊 Descubre la confianza digital"
            },
            elite: {
                tagline: "Precisión y poder ejecutivo",
                description: "Represento la máxima elegancia corporativa y precisión estratégica. Con mi mirada penetrante y estilo minimalista, lidero comunicaciones de alto nivel con resultados impecables.",
                cta: "⚫ Experimenta la excelencia"
            }
        };

        function changeTheme(themeName) {
            const container = document.getElementById('mainContainer');
            const tagline = document.getElementById('tagline');
            const description = document.getElementById('description');
            const cta = document.getElementById('cta');
            
            // Remover todas las clases de tema
            container.classList.remove('theme-nature', 'theme-ocean', 'theme-elite');
            
            // Añadir nueva clase de tema
            container.classList.add(`theme-${themeName}`);
            
            // Actualizar contenido
            tagline.textContent = themes[themeName].tagline;
            description.textContent = themes[themeName].description;
            cta.textContent = themes[themeName].cta;
            
            // Actualizar botones activos
            const buttons = document.querySelectorAll('.version-btn');
            buttons.forEach(btn => btn.classList.remove('active'));
            document.querySelector(`.version-btn.${themeName}`).classList.add('active');
        }

        // Añadir interactividad al avatar
        const avatar = document.querySelector('.avatar');
        const eyes = document.querySelectorAll('.eye');
        
        if (avatar && eyes.length > 0) {
            avatar.addEventListener('mouseover', function() {
                eyes.forEach(eye => {
                    eye.style.transform = 'scale(1.1)';
                });
            });
            
            avatar.addEventListener('mouseout', function() {
                eyes.forEach(eye => {
                    eye.style.transform = 'scale(1)';
                });
            });

            // Efecto de parpadeo
            setInterval(() => {
                eyes.forEach(eye => {
                    eye.style.height = '3px';
                    setTimeout(() => {
                        eye.style.height = '25px';
                    }, 150);
                });
            }, 4000);
        }
    </script>

</body>
